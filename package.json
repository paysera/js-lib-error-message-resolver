    },
    "bugs": {
        "url": "https://github.com/paysera/js-lib-error-message-resolver/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "author": "Paysera",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/plugin-transform-runtime": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@paysera/eslint-config": "^1.0.0",
        "babel-jest": "^24.1.0",
        "babel-loader": "^8.0.5",
        "coveralls": "^3.0.3",
        "eslint": "^5.3.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "i18next-parser": "1.0.0-beta34",
        "jest": "^24.1.0",
        "rimraf": "^2.6.3",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "i18next": "^18.0.0",
        "lodash": "^4.17.11",
        "moment": "^2.24.0"
    }
